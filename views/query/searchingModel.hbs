<h1>All cars for rent</h1>

<form action="/searchingModel" method="GET">
  <fieldset>
    <legend>Search by model:</legend>

    <label for="name">Model:</label>
    <input type="text" name="model">
    <input type="submit"><br>
</fieldset>
</form>

    {{#each searchedCars}}
        <h3>Model:</h3>
        <p>{{this.newModel}}</p>

        <h3>vehNo:</h3>
        <p>{{this.vehNo}}</p>

        <h3>Price:</h3>
        <p>{{this.price}}</p>

        <h3>capacity:</h3>
        <p>{{this.capacity}}</p>
        <a href="/carDetails/{{this._id}}"><b>TAKE IT THIS CAR</b></a><br>
        <a href="/rmCar/{{this._id}}"><b> Remove car</b></a>
        <br>

    {{/each}}
